\par {\listtext\pard\plain\s3 \i\fs20\lang1033\langfe1033\loch\af1\hich\af1\dbch\af0\langfenp1033 \hich\af1\dbch\af0\loch\f1 1.2.1\tab}}\pard\plain \s3\ql \li0\ri0\sb120\sa60\sl240\slmult0\keepn\nowidctlpar
