<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="ProjectCategoryLookup" table="project_category_lu">
		<id access="field" column="project_category_id" name="projectCategoryId"
			type="long" />
		<property access="field" column="project_group_category_id"
			name="projectGroupCategoryId" type="long" />
		<property access="field" column="name" name="name"
			type="java.lang.String" />
	</class>
</hibernate-mapping>
