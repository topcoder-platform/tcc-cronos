<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="ResourceInfo" table="resource_info">
		<composite-id>
			<key-property access="field" column="resource_id" name="resourceId"
				type="long" />
			<key-property access="field" column="resource_info_type_id"
				name="resourceInfoTypeId" type="long" />
		</composite-id>
		<property access="field" column="value" name="value"
			type="java.lang.String" />
	</class>
</hibernate-mapping>
