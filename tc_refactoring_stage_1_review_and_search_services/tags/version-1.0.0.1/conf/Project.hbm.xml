<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.web.tc.impl.entity">
	<class name="Project" table="project">
		<id access="field" column="project_id" name="projectId" type="long" />
		<property access="field" column="project_category_id" name="projectCategoryId"
			type="long" />
		<property access="field" column="project_status_id" name="projectStatusId"
			type="long" />
		<property access="field" column="tc_direct_project_id" name="tcDirectProjectId"
			type="long" />
	</class>
</hibernate-mapping>
