<?xml version="1.0"?>
<CMConfig>

  <!-- No SQL query template -->
  <Config name="BadDatabaseSearchUtility1">
    <Property name="column_aliases">
      <Property name="Name">
        <Value>Clients.Name</Value>
      </Property>
    </Property>
  </Config>

  <!-- No aliases of the DB column names -->
  <Config name="BadDatabaseSearchUtility2">
    <Property name="query_template">
      <Value>select distinct Clients.ClientsID from Clients where </Value>
    </Property>
  </Config>

  <!-- No value in the aliases of the DB column names -->
  <Config name="BadDatabaseSearchUtility3">
    <Property name="query_template">
      <Value>select distinct Clients.ClientsID from Clients where </Value>
    </Property>
    <Property name="column_aliases">
      <Property name="Name">
      </Property>
    </Property>
  </Config>

  <!-- No class name of the persistence -->
  <Config name="BadProjectPersistenceManager1">
    <Property name="connection_factory_namespace">
      <Value>com.topcoder.db.connectionfactory</Value>
    </Property>
  </Config>

  <!-- No namespace of the DB Connection Factory configuration file -->
  <Config name="BadProjectPersistenceManager2">
    <Property name="persistence_class">
      <Value>com.cronos.timetracker.project.persistence.InformixTimeTrackerProjectPersistence</Value>
    </Property>
  </Config>

  <!-- No connection producer name, but with project and client search utilty namespaces -->
