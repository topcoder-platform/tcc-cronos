						<Value>com.topcoder.timetracker.audit.persistence.InformixAuditPersistence</Value>
					</Property>
				</Property>
			</Property>
		</Property>
	</Config>

	<Config name="com.topcoder.timetracker.audit.persistence.DefaultValuesContainer">
		<Property name="id">
			<Value>100</Value>
		</Property>
		<Property name="newValue">
			<Value>newValue</Value>
		</Property>
		<Property name="oldValue">
			<Value>oldValue</Value>
		</Property>
		<Property name="columnName">
			<Value>columnName</Value>
		</Property>
	</Config>

	<Config name="com.topcoder.timetracker.audit.persistence.InformixAuditPersistence">
		<Property name="connectionName">
			<Value>tt_time_entry</Value>
		</Property>
		<Property name="connectionFactoryNamespace">
			<Value>com.topcoder.db.connectionfactory.DBConnectionFactoryImpl</Value>
		</Property>
		<Property name="logName">
			<Value>InformixAuditPersistence</Value>
		</Property>
		<Property name="idGeneratorName">
			<Value>AuditIdGenerator</Value>
		</Property>
		<Property name="searchBundleName">
			<Value>FirstSearchBundle</Value>
		</Property>
		<Property name="searchBundleNamespace">
			<Value>com.topcoder.search.builder.audit</Value>
		</Property>
		<Property name="defaultValueNamespace">
			<Value>com.topcoder.timetracker.audit.persistence.DefaultValuesContainer</Value>
		</Property>
