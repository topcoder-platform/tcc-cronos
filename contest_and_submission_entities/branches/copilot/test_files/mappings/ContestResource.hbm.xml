<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.service.studio.contest">
  <class name="ContestResource" table="resource" lazy="false">
    <id name="resourceId" column="resource_id" type="long"
      unsaved-value="-1">
      <generator class="sequence">
        <param name="sequence">CONTEST_RESOURCE_SEQ</param>
      </generator>
    </id>


    <property name="name" column="resource_name" not-null="false" />


  </class>


</hibernate-mapping>

