<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.direct.services.project.metadata.entities.dao">
  <class name="DirectProjectMetadataAudit" table="direct_project_metadata_audit">
    <id name="id" column="project_metadata_audit_id">
			<generator class="increment"/>
    </id>

    <property name="projectMetadataId" column="project_metadata_id" not-null="true" />
    <property name="tcDirectProjectId" column="tc_direct_project_id" not-null="true" />
    <property name="projectMetadataKeyId" column="project_metadata_key_id" not-null="true" />
    <property name="metadataValue" column="metadata_value" not-null="true" />
    <property name="auditActionTypeId" column="audit_action_type_id" not-null="true"/>
    <property name="actionDate" column="action_date" not-null="true"/>
    <property name="actionUserId" column="action_user_id" not-null="true"/>
  </class>
</hibernate-mapping>
