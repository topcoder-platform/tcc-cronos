<project name="Dependency Import" default="dist" basedir=".">
    <!-- Properties used in compile_targets ANT target -->
    <property name="component.target" value="1.5"/>
    <property name="component.bootclasspath" value="${java_1_5_bootclasspath}"/>
    
    <!-- property file defining the component's dependencies -->

    <!-- define TCS dependencies -->
    <property name="base_exception.jar" value="${tcs_libdir}/base_exception/2.0.0/base_exception.jar"/>
    <property name="configuration_manager.jar" value="${tcs_libdir}/configuration_manager/2.1.5/configuration_manager.jar"/>
    <property name="logging_wrapper.jar" value="${tcs_libdir}/logging_wrapper/2.0.0/logging_wrapper.jar"/>
    <property name="object_factory.jar" value="${tcs_libdir}/object_factory/2.1.0/object_factory.jar"/>
    <property name="object_factory_config_manager_plugin.jar"
     value="${tcs_libdir}/object_factory_config_manager_plugin/1.0.0/object_factory_config_manager_plugin.jar"/>
    <property name="project_phases.jar" value="${tcs_libdir}/project_phases/2.0/project_phases.jar"/>   
    <property name="phase_management.jar" value="${tcs_libdir}/phase_management/1.0.4/phase_management.jar"/>
    <property name="document_generator.jar" value="${tcs_libdir}/document_generator/2.1/document_generator.jar"/>
    <property name="email_engine.jar" value="${tcs_libdir}/email_engine/3.0/email_engine.jar"/>

    <property name="submission_manager.jar" value="${cronos_libdir}/submission_manager/1.1.0/submission_manager.jar"/>
    <property name="studio_contest_manager.jar" value="${cronos_libdir}/studio_contest_manager/1.2.0/studio_contest_manager.jar"/>
    <property name="contest_and_submission_entities.jar" value="${cronos_libdir}/contest_and_submission_entities/1.1.0/contest_and_submission_entities.jar"/>
    
    <!-- define internal party dependencies -->
    <property name="forums.jar" value="${cockpit_int_libdir}/forums/1.0.0/forums.jar"/>

    <!-- define 3rd party dependencies -->
    <property name="jboss_lib" value="${ext_libdir}/jboss/as/4.2.2.ga/server/all/lib/"/>
    <property name="activation.jar" value="${jboss_lib}/activation.jar"/>
    <property name="jboss-j2ee.jar" value="${jboss_lib}/jboss-j2ee.jar"/>
    <property name="jboss-ejb3x.jar" value="${jboss_lib}/jboss-ejb3x.jar"/>
    <property name="mail.jar" value="${ext_libdir}/javamail/1.3.3/mail.jar"/>
    <property name="log4j.jar" value="${ext_libdir}/log4j/1.2.9/log4j-1.2.9.jar"/>
    <property name="easymock.jar" value="${ext_libdir}/easymock/2.3/easymock.jar"/>
    <property name="junit.jar" value="${ext_libdir}/junit/3.8.2/junit.jar"/>
        
    <path id="component.tcs-dependencies">
        <pathelement location="${base_exception.jar}"/>
        <pathelement location="${configuration_manager.jar}"/>
        <pathelement location="${object_factory.jar}"/>
        <pathelement location="${object_factory_config_manager_plugin.jar}"/>
        <pathelement location="${project_phases.jar}"/>        
        <pathelement location="${phase_management.jar}"/>
        <pathelement location="${document_generator.jar}"/>
        <pathelement location="${email_engine.jar}"/>        
        <pathelement location="${forums.jar}"/>
        <pathelement location="${submission_manager.jar}"/>
        <pathelement location="${logging_wrapper.jar}"/>
    </path>
    <path id="component.3rdParty-dependencies">
        <pathelement location="${activation.jar}"/>
        <pathelement location="${mail.jar}"/>
        <pathelement location="${log4j.jar}"/>
        <pathelement location="${jboss-j2ee.jar}"/>
        <pathelement location="${jboss-ejb3x.jar}"/>
        <pathelement location="${studio_contest_manager.jar}"/>
        <pathelement location="${contest_and_submission_entities.jar}"/>
    </path>
    <path id="component.test.3rdParty-dependencies">
        <pathelement location="${junit.jar}"/>
        <pathelement location="${easymock.jar}"/>
    </path>

</project>
