<?xml version="1.0" encoding="UTF-8" ?>
<ConfigManager>
    <namespace name="Orpheus.Plugin.Toolbar">
	    <!-- The URL of the application -->
        <property name="context">
            <value>http://www.theball.com</value>
		</property>
	    
	    <!-- The login URL -->
        <property name="login_url">
            <value>/plugin/login.jsp</value>
		</property>
	    <!-- The logout URL -->
        <property name="logout_url">
            <value>/server/plugin/logout.do</value>
		</property>
	    <!-- The Games page URL -->
        <property name="games_url">
            <value>/server/plugin/activeGames.do</value>
		</property>
	    <!-- The unlocked sites URL -->
        <property name="unlocked_url">
            <value>/server/plugin/unlockedDomains.do?gameId={0}</value>
		</property>
	    
	    <!-- The upcoming sites URL -->
        <property name="upcoming_url">
            <value>/server/plugin/upcomingDomains.do?gameId={0}</value>
		</property>
		
		<!-- The leaderboard URL -->
        <property name="leaderboard_url">
            <value>/server/plugin/leaders.do?gameId={0}</value>
		</property>
	</namespace>

