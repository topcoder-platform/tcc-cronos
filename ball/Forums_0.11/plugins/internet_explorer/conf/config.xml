        </property>
    </namespace>
    
    <namespace name="TopCoder.Util.ObjectFactory.test_object">
        <property name="type_name">
            <value>Orpheus.Plugin.InternetExplorer.Handlers.OrpheusTestObjectEventHandler,Orpheus.Plugin, Version=1.0.0.0, Culture=neutral, PublicKeyToken=25404110f0acce22</value>
        </property>
    </namespace>
    
    <namespace name="TopCoder.Util.ObjectFactory.test_object_url">
        <property name="type_name">
            <value>Orpheus.Plugin.InternetExplorer.EventsManagers.Handlers.TargetURLEventHandler,Orpheus.Plugin.InternetExplorer, Version=1.0.0.0, Culture=neutral, PublicKeyToken=25404110f0acce22</value>
