        </property>
    </namespace>

    <namespace name="TopCoder.Util.ObjectFactory.test_domain">
        <property name="type_name">
            <value>Orpheus.Plugin.InternetExplorer.Handler.ForcedDominTestEventHandler,Orpheus.Plugin, Version=1.0.0.0, Culture=neutral, PublicKeyToken=25404110f0acce22</value>
        </property>
    </namespace>

