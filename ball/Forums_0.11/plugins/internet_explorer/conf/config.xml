            <value>Orpheus.Plugin.InternetExplorer.WindowNavigators.DefaultWebBrowserWindowNavigator,Orpheus.Plugin.InternetExplorer, Version=1.0.0.0, Culture=neutral, PublicKeyToken=25404110f0acce22</value>
        </property>
    </namespace>
    <namespace name="TopCoder.Util.ObjectFactory.window_navigator.parameters">
        <property name="ns:string">
            <value>Orpheus.Plugin.InternetExplorer.WindowNavigators</value>
        </property>
    </namespace>
    
    <!-- This namespace contains configuration of the popup window appearance -->
    <namespace name="Orpheus.Plugin.InternetExplorer.WindowNavigators">
        <property name="window_width"><value>800</value></property>
        <property name="window_height"><value>500</value></property>
        <property name="toolbar"><value>0</value></property>
        <property name="statusbar"><value>false</value></property>
        <property name="menubar"><value>false</value></property>
        <property name="addressbar"><value>false</value></property>
        <property name="allow_window_close"><value>true</value></property>
    </namespace>
    
    

