<action name="ShowFAQPage" type="default" url-pattern="/server/player/showPlayerFAQ.do" priority="10">
    <handler type="SessionValidationHandler">
        <login_page_result>login</login_page_result>
    </handler>
    <handler type="authorization">
        <allowed_profile_types>
            <value>player</value>
        </allowed_profile_types>
        <failure_result>auth_failed</failure_result>
    </handler>
    <result name="auth_failed" type="dbLoggingForward">
        <forward-url>/public/authorizationError.jsp</forward-url>
        <log-message>attempted to view Player FAQ page without PLAYER role assigned</log-message>
        <category>WEB SITE</category>
        <action-code>39</action-code>
    </result>
    <result name="success" type="dbLoggingForward">
        <forward-url>/WEB-INF/player/player-faq.jsp</forward-url>
        <log-message>presented with Player FAQ page</log-message>
        <category>WEB SITE</category>
        <action-code>128</action-code>
    </result>
    <result name="login" type="dbLoggingForward">
        <forward-url>/public/timeout.jsp</forward-url>
        <log-message>failed to open Player FAQ page due to session timeout or is not authenticated to server
        </log-message>
        <category>WEB SITE</category>
        <action-code>60</action-code>
    </result>
</action>