    <!-- global results that will be added into all configured actions. -->
    <global-results>
        <result name="gameCompleted-error" type="forward">
            <forward-url>/WEB-INF/plugin/alert-gamecompleted.jsp</forward-url>
        </result>
        <result name="notHostingSlot-error" type="loggingForward">
            <forward-url>/WEB-INF/plugin/alert-nothostingslot.jsp</forward-url>
            <log-message>the site {0} is not currently in the hosting sequence for game {1}</log-message>
            <parameters>domain,gameId</parameters>
        </result>
    </global-results>
