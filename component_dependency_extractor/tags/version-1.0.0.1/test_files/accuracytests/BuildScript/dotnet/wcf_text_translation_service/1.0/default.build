<?xml version="1.0"?>
<project name="WCF Text Translation Service" default="compile" basedir=".">

  <tstamp property="build.date" pattern="dd-MM-yyyy" verbose="true"/>
  <sysinfo verbose="true"/>

  <property name="main_project_folder" value="TopCoder.Services.WCF.TextTranslation" />
  <property name="component" value="WCF_Text_Translation_Service" />
  <property name="distfilename" value="wcf_text_translation_service" />
  <property name="namespace_file" value="TopCoder.Services.WCF.TextTranslation" />

  <property name="component_version" value="1.0" />

  <property name="copyright" value="Copyright (c) 2007, TopCoder, Inc. All rights reserved." />

  <!--  REFERENCE TO NUNIT -->
  <property name="Nunit" value="C:\Program Files\NUnit-Net-2.0 2.2.9\bin\nunit.framework.dll"/>
  <property name="NunitConsole" value="C:\Program Files\NUnit-Net-2.0 2.2.9\bin\nunit-console.exe"/>
  <property name="NDoc" value="C:\Program Files\NDoc\bin\.net-1.1\NDocConsole.exe"/>

