  <property name="LoggingWrapper.dll" value="${tcs_bin}\logging_wrapper\2.0\TopCoder.LoggingWrapper.dll" />
  <property name="ExceptionManager.dll" value="${tcs_bin}\exception_manager\1.0\TopCoder.Util.ExceptionManager.dll" />
  <property name="SelfDocumentingException.dll" value="${tcs_bin}\self-documenting_exception\1.0\TopCoder.Util.ExceptionManager.SDE.dll" />
  <property name="ConfigurationAPI.dll" value="${tcs_bin}\configuration_api\1.0\TopCoder.Configuration.dll" />
