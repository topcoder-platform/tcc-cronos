            <Property name="ClientDaoSearchBundle">
                <Property name="searchStrategy">
                    <Property name="class">
                        <Value>dbStrategy</Value>
                    </Property>
                </Property>
                <Property name="context">
                    <Value>
                        SELECT distinct
                        client.client_id
                        FROM
						client INNER JOIN client_project On
						client.client_id = client_project.client_id
                        WHERE
                    </Value>
                </Property>
                <Property name="alias">
                    <Property name="ALL">
                        <Value>*</Value>
                    </Property>
                    <Property name="ID">
                        <Value>client.client_id</Value>
					</Property>
					<Property name="CLIENT_ID">
