        <Property name="dbStrategy">
            <Property name="type">
                <Value>com.topcoder.search.builder.database.DatabaseSearchStrategy</Value>
            </Property>
            <Property name="params">
                <Property name="param1">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
