					</Property>
					<Property name="NAME">
						<Value>client.name</Value>
					</Property>
					<Property name="GREEK_NAME">
						<Value>client.greek_name</Value>
					</Property>
					<Property name="PROJECT_ID">
						<Value>client_project.project_id</Value>
					</Property>
                </Property>
