                    </Property>
                    <Property name="END_DATE">
                        <Value>client.end_date</Value>
                    </Property>
                    <Property name="START_DATE">
                        <Value>client.start_date</Value>
					</Property>
					<Property name="MODIFICATION_DATE">
						<Value>client.modification_date</Value>
					</Property>
					<Property name="MODIFICATION_USER">
						<Value>client.modification_user</Value>
					</Property>
					<Property name="CREATION_DATE">
						<Value>client.creation_date</Value>
					</Property>
					<Property name="CREATION_USER">
