<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
    version="3.0">
    <enterprise-beans>
        <session>
            <ejb-name>ContestManagerBean</ejb-name>
            <env-entry>
                <env-entry-name>unitName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>persistence/contest_submission</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>activeContestStatusId</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>defaultDocumentPathId</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>loggerName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>contestManager</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>documentContentManagerClassName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>com.topcoder.service.studio.contest.documentcontentmanagers.FileSystemDocumentContentManager</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>documentContentManagerAttributeKeys</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>path</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>path</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/</env-entry-value>
            </env-entry>
            <env-entry> 
                <env-entry-name>auditChange</env-entry-name> 
                <env-entry-type>java.lang.Boolean</env-entry-type> 
                <env-entry-value>false</env-entry-value> 
            </env-entry>        
            <env-entry>
                <env-entry-name>submitter_terms_id</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>20943</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>submitter_role_id</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
            <persistence-context-ref>
                <persistence-context-ref-name>persistence/contest_submission</persistence-context-ref-name>
                <persistence-unit-name>contest_submission</persistence-unit-name>                     
            </persistence-context-ref>
        </session>
    </enterprise-beans>
</ejb-jar>
