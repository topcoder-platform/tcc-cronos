<?xml version="1.0"?>
<CMConfig>
        <!-- Configuration for ServiceEngine-->
        <Config name="com.topcoder.service.ServiceEngine">
                <Property name="object_factory">
                        <Value>com.topcoder.util.objectfactory.serviceengine</Value>
                </Property>
                <Property name="service_linker">
                        <Value>default_service_linker</Value>
                </Property>
                <Property name="element_selector">
                        <Value>default_element_selector</Value>
                </Property>
                <Property name="service_engine_persistence">
                        <Value>default_service_engine_persistence</Value>
                </Property>
                <Property name="requester_persistence">
                        <Value>default_requester_persistence</Value>
                </Property>
                <Property name="responder_persistence">
                        <Value>default_responder_persistence</Value>
                </Property>
                <Property name="service_handlers">
                        <Property name="category_1">
                                <Value>default_service_handler</Value>
                        </Property>
                        <Property name="category_2">
                                <Value>default_service_handler</Value>
                        </Property>
                        <Property name="category_3">
                                <Value>default_service_handler</Value>
                        </Property>
                </Property>
        </Config>

        <!-- Configuration for ObjectFactory-->
        <Config name="com.topcoder.util.objectfactory.serviceengine">
                <!-- Configuration for service linker -->
                <Property name="default_service_linker">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceLinker</Value>
                        </Property>
                </Property>
                <!-- Configuration for element selector -->
                <Property name="default_element_selector">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceElementSelector</Value>
                        </Property>
                </Property>
                <!-- Configuration for default service handler -->
                <Property name="default_service_handler">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceHandler</Value>
                        </Property>
                </Property>
                <!-- Configuration for service engine persistence -->
                <Property name="default_service_engine_persistence">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceEnginePersistence</Value>
                        </Property>
                </Property>
                <!-- Configuration for requester persistence -->
                <Property name="default_requester_persistence">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceElementPersistence</Value>
                        </Property>
                </Property>
                <!-- Configuration for responder persistence -->
                <Property name="default_responder_persistence">
                        <Property name="type">
                                <Value>com.cronos.im.logic.MockServiceElementPersistence</Value>
                        </Property>
                </Property>
                <!-- Configuration for category-1 -->
                <Property name="category_1">
                        <Property name="type">
                                <Value>com.topcoder.service.Category</Value>
                        </Property>
                        <Property name="params">
                                <Property name="param1">
                                        <Property name="type">
                                                <Value>long</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>1</Value>
                                        </Property>
                                </Property>
                                <Property name="param2">
                                        <Property name="type">
                                                <Value>String</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>TEACHER_ONLY</Value>
                                        </Property>
                                </Property>
                        </Property>
                </Property>
                <!-- Configuration for category-2 -->
                <Property name="category_2">
                        <Property name="type">
                                <Value>com.topcoder.service.Category</Value>
                        </Property>
                        <Property name="params">
                                <Property name="param1">
                                        <Property name="type">
                                                <Value>long</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>2</Value>
                                        </Property>
                                </Property>
                                <Property name="param2">
                                        <Property name="type">
                                                <Value>String</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>STUDENT_ONLY</Value>
                                        </Property>
                                </Property>
                        </Property>
                </Property>
                <!-- Configuration for category-3 -->
                <Property name="category_3">
                        <Property name="type">
                                <Value>com.topcoder.service.Category</Value>
                        </Property>
                        <Property name="params">
                                <Property name="param1">
                                        <Property name="type">
                                                <Value>long</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>3</Value>
                                        </Property>
                                </Property>
                                <Property name="param2">
                                        <Property name="type">
                                                <Value>String</Value>
                                        </Property>
                                        <Property name="value">
                                                <Value>NORMAL</Value>
                                        </Property>
                                </Property>
                        </Property>
                </Property>
        </Config>

</CMConfig>