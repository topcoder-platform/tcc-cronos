<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"       "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd"><validators>  <field name="invitationId">      <field-validator type="required">          <message>Invitation ID must be specified.</message>      </field-validator>      <field-validator type="long">			<param name="min">1</param>			<message>Invitation ID must be long.</message>      </field-validator>  </field>    <field name="accepted">      <field-validator type="required">          <message>Accepted flag must be specified.</message>      </field-validator>  </field></validators>