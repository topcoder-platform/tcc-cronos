<?xml version="1.0"?>
<CMConfig>
  <Config name="com.cronos.im.ajax.objectfactory">
	<Property name="messenger_key">
		<Property name="type">
			<Value>com.cronos.im.ajax.accuracytests.MessengerTester</Value>
		</Property>
	</Property>
	<Property name="chat_session_manager_key">
		<Property name="type">
			<Value>com.cronos.im.ajax.accuracytests.ChatSessionManagerTester</Value>
		</Property>
	</Property>
	<Property name="chat_status_tracker_key">
		<Property name="type">
			<Value>com.cronos.im.ajax.accuracytests.ChatStatusTrackerTester</Value>
		</Property>
	</Property>
	<Property name="service_engine_key">
		<Property name="type">
			<Value>com.topcoder.service.ServiceEngine</Value>
		</Property>
	</Property>

	<Property name="AcceptChatRequestHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.AcceptChatRequestHandler</Value>
		</Property>
	</Property>
	<Property name="ChangeManagerStatusHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.ChangeManagerStatusHandler</Value>
		</Property>
	</Property>
	<Property name="PostTextMessageHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.PostTextMessageHandler</Value>
		</Property>
	</Property>
	<Property name="ReadClientSessionMessageHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.ReadClientSessionMessageHandler</Value>
		</Property>
	</Property>
	<Property name="ReadClientUserMessageHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.ReadClientUserMessageHandler</Value>
		</Property>
	</Property>
	<Property name="ReadManagerSessionMessageHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.ReadManagerSessionMessageHandler</Value>
		</Property>
	</Property>
	<Property name="ReadManagerUserMessageHandler">
		<Property name="type">
			<Value>com.cronos.im.ajax.handler.ReadManagerUserMessageHandler</Value>
		</Property>
	</Property>
  </Config>
  <Config name="com.topcoder.util.log">
	<!-- The Log implementation class -->
	<Property name="logClass">
		<Value>com.topcoder.util.log.basic.BasicLog</Value>
	</Property>

	<!-- Specifies where the output should be written to, System.out or System.err -->
	<Property name="basic.log.target">
		<Value>System.out</Value>
	</Property>
  </Config>
	<Config name="com.cronos.im.ajax.IMAjaxSupportUtility">

<!-- The client role name. Required. -->
		<Property name="client_role_name">
			<Value>client</Value>
		</Property>
<!-- The manager role name Required. -->
		<Property name="manager_role_name">
			<Value>manager</Value>
		</Property>
<!-- The property name used to get the role name from the user profile. Required. -->
		<Property name="role_property_name">
			<Value>role</Value>
		</Property>
<!-- The key used to get the ChatUserProfile from the http session attribute. Required. -->
		<Property name="user_profile_session_key">
			<Value>user_profile_session_key</Value>
		</Property>
<!-- The object key used to create the Messenger from object factory Required. -->
		<Property name="messenger_key">
			<Value>messenger_key</Value>
		</Property>
<!-- The object key used to create the ChatSessionManager from object factory Required. -->
		<Property name="chat_session_manager_key">
			<Value>chat_session_manager_key</Value>
		</Property>
<!-- The object key used to create the ChatStatusTracker from object factory Required. -->
		<Property name="chat_status_tracker_key">
			<Value>chat_status_tracker_key</Value>
		</Property>
<!-- The object key used to create the ServiceEngine from object factory Required. -->
		<Property name="service_engine_key">
			<Value>service_engine_key</Value>
		</Property>
<!-- The cookie name used to get the time zone. Required. -->
		<Property name="time_zone_cookie_name">
			<Value>timeZone</Value>
		</Property>


<!-- Get the category key used to get the category from http session attribute -->
		<Property name="category_session_key">
			<Value>category_session_key</Value>
		</Property>
<!-- The key used to get the xml request message from the http request -->
		<Property name="xml_request">
			<Value>xml_request</Value>
		</Property>
<!-- the key used to get the user id from ServiceEelement -->
		<Property name="user_id_property_key">
			<Value>user_id_property_key</Value>
		</Property>
<!-- The key used to get the session id from ServiceEelement -->
		<Property name="session_id_property_key">
			<Value>session_id_property_key</Value>
		</Property>

	</Config>
	<Config name="com.cronos.im.ajax.RequestHandlerManager">
<!-- namespace to create object factory -->
		<Property name="object_factory_namespace">
			<Value>com.cronos.im.ajax.objectfactory</Value>
		</Property>

<!-- Contains the type values. Required. -->
		<Property name="handler_types">
			<Value>AcceptChatRequest</Value>
			<Value>ChangeManagerStatus</Value>
			<Value>PostTextMessage</Value>
			<Value>ReadClientSessionMessage</Value>
			<Value>ReadClientUserMessage</Value>
			<Value>ReadManagerSessionMessage</Value>
			<Value>ReadManagerUserMessage</Value>
		</Property>
<!-- each of the type values defined in "handler_types" property. The value of the xxx property
 is object key used to create the handler instance from object factory. Required. -->
		<Property name="AcceptChatRequest">
			<Value>AcceptChatRequestHandler</Value>
		</Property>
		<Property name="ChangeManagerStatus">
			<Value>ChangeManagerStatusHandler</Value>
		</Property>
		<Property name="PostTextMessage">
			<Value>PostTextMessageHandler</Value>
		</Property>
		<Property name="ReadClientSessionMessage">
			<Value>ReadClientSessionMessageHandler</Value>
		</Property>
		<Property name="ReadClientUserMessage">
			<Value>ReadClientUserMessageHandler</Value>
		</Property>
		<Property name="ReadManagerSessionMessage">
			<Value>ReadManagerSessionMessageHandler</Value>
		</Property>
		<Property name="ReadManagerUserMessage">
			<Value>ReadManagerUserMessageHandler</Value>
		</Property>
	</Config>
</CMConfig>