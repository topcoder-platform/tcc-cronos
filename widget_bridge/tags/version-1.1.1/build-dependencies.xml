<?xml version="1.0" encoding="UTF-8"?>
<project basedir="."  name="Dependency Import">
    <!--property file defining the component's dependencies-->
    <!--Properties used in compile_targets ANT target for JDK version x-->
    <property name="component.target" value="1.5"/>
    <property name="component.bootclasspath" value="${java_1_5_bootclasspath}"/>
    
    <!--TCS dependencies-->
    <property name="json_object.jar" value="${tcs_libdir}/json_object/1.0/json_object.jar"/>
    <property name="configuration_api.jar"  value="${tcs_libdir}/configuration_api/1.0/configuration_api.jar"/>
    <property name="configuration_manager.jar" value="${tcs_libdir}/configuration_manager/2.1.5/configuration_manager.jar"/>
    <property name="configuration_persistence.jar" value="${tcs_libdir}/configuration_persistence/1.0.1/configuration_persistence.jar"/>
    <property name="object_factory.jar" value="${tcs_libdir}/object_factory/2.1.0/object_factory.jar"/>
    <property name="object_factory_configuration_api_plugin.jar"  value="${tcs_libdir}/object_factory_configuration_api_plugin/1.0/object_factory_configuration_api_plugin.jar"/>
    <property name="logging_wrapper.jar" value="${tcs_libdir}/logging_wrapper/2.0/logging_wrapper.jar"/>
    <property name="file_system_server.jar" value="${tcs_libdir}/file_system_server/1.0/file_system_server.jar"/>
    <property name="file_upload.jar" value="${tcs_libdir}/file_upload/2.2.0/file_upload.jar"/>
    <property name="guid_generator.jar" value="${tcs_libdir}/guid_generator/1.0/guid_generator.jar"/>
    <property name="ip_server.jar" value="${tcs_libdir}/ip_server/2.0.1/ip_server.jar"/>
    <property name="base_exception.jar" value="${tcs_libdir}/base_exception/2.0/base_exception.jar"/>
    <property name="object_formatter.jar" value="${tcs_libdir}/object_formatter/1.0/object_formatter.jar"/>
    <property name="typesafe_enum.jar" value="${tcs_libdir}/typesafe_enum/1.1/typesafe_enum.jar"/>
    <property name="search_builder.jar" value="${tcs_libdir}/search_builder/1.3.2/search_builder.jar"/>
    <property name="project_service.jar" value="${cockpit_tcs_libdir}/project_service/x.x.x/project_service.jar"/>

    <!-- Service dependencies -->
    <property name="studio_service.jar" value="${cronos_libdir}/studio_service/1.2.0/studio_service.jar"/>
    <property name="prerequisite_service.jar" value="${cronos_libdir}/prerequisite_service/1.0.0/prerequisite_service.jar"/>
    
    <!--3rd party dependencies-->
    <property name="junit.jar" value="${ext_libdir}/junit/3.8.1/junit.jar"/>
    
    <property name="spring.jar" value="${ext_libdir}/spring/2.5.4/spring.jar"/>
    <property name="spring-test.jar" value="${ext_libdir}/spring/2.5.4/spring-test.jar"/>

    <path id="jboss.classpath">
      <fileset dir="${jboss_lib}">
        <include name="jboss-ejb3x.jar" />
        <include name="jboss-j2ee.jar" />
        <include name="commons-logging.jar" />
        <include name="servlet-api.jar" />
      </fileset>
    </path>

    <property name="jbossws.dir" value="${ext_libdir}/jboss/as/4.2.2.ga/client/" />    
    <path id="jbossws.classpath">
      <fileset dir="${jbossws.dir}">
        <include name="jaxb-api.jar" />
        <include name="jboss-jaxws.jar" />
      </fileset>    
    </path>
    
    <!--Path declarations which are to be used in the master build file.-->
    <path id="component.tcs-dependencies">
        <pathelement location="${json_object.jar}"/>
        <pathelement location="${configuration_api.jar}"/>
        <pathelement location="${configuration_persistence.jar}"/>
        <pathelement location="${object_factory.jar}"/>
        <pathelement location="${object_factory_configuration_api_plugin.jar}"/>        
        <pathelement location="${logging_wrapper.jar}"/>
        <pathelement location="${file_upload.jar}"/>
        <pathelement location="${base_exception.jar}"/>
        <pathelement location="${object_formatter.jar}"/>
        <pathelement location="${typesafe_enum.jar}"/>
        <pathelement location="${studio_service.jar}"/>
        <pathelement location="${project_service.jar}"/>
        <pathelement location="${prerequisite_service.jar}"/>      
        <pathelement location="${search_builder.jar}"/>
    </path>
    <path id="component.3rdParty-dependencies">
        <path refid="jboss.classpath"/>
         <path refid="jbossws.classpath"/> 
    </path>
    <path id="component.test.3rdParty-dependencies">
        <pathelement location="${junit.jar}"/>
        <pathelement location="${spring.jar}"/>
        <pathelement location="${spring-test.jar}"/>        
    </path>
</project>
