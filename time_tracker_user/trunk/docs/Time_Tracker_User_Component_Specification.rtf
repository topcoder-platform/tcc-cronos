\par {\pntext\pard\plain\f3\fs20\cf6\lang1033\langfe1033\langfenp1033\insrsid13136598 \loch\af3\dbch\af13\hich\f3 \'b7\tab}}\pard \ql \fi-720\li1440\ri0\nowidctlpar{\*\pn \pnlvlblt\ilvl0\ls1\pnrnot0\pnf3 {\pntxtb \hich \'b7}}
