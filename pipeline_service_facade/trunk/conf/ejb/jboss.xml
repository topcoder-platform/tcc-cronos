<?xml version="1.0"?>
<jboss>
	<enterprise-beans>
		<session>
			<ejb-name>PipelineServiceFacadeBean</ejb-name>
			<jndi-name>remote/PipelineServiceFacadeBean</jndi-name>

			<ejb-ref>
				<ejb-ref-name>ejb/ContestPipelineService</ejb-ref-name>
				<jndi-name>remote/ContestPipelineServiceBean</jndi-name>
			</ejb-ref>

			<port-component>
				<port-component-name>PipelineServiceFacadeBean</port-component-name>
				<port-component-uri>/PipelineServiceFacadeBean</port-component-uri>
				<transport-guarantee>CONFIDENTIAL</transport-guarantee>
			</port-component>
		</session>
	</enterprise-beans>

	<security-domain>java:/jaas/JBossLoginModule</security-domain>
	<webservices>
		<context-root>/pipelinefacade</context-root>
		<webservice-description>
			<webservice-description-name />
			<wsdl-publish-location />
		</webservice-description>
	</webservices>
</jboss>
