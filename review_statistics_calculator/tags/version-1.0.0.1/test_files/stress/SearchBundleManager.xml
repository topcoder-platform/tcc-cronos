					FROM resource LEFT OUTER JOIN project ON resource.project_id = project.project_id 
					LEFT OUTER JOIN resource_submission ON resource.resource_id = resource_submission.resource_id 
					LEFT OUTER JOIN resource_info ON resource.resource_id = resource_info.resource_id 
					LEFT OUTER JOIN resource_info_type_lu ON resource_info.resource_info_type_id = resource_info_type_lu.resource_info_type_id 
