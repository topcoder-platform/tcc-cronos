<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
    version="3.0">

 <enterprise-beans>

   <session>
     <ejb-name>ResourceManagerServiceBean</ejb-name>
     <ejb-class>com.topcoder.registration.persistence.ejbservice.ResourceManagerServiceBean</ejb-class>
     <env-entry>
       <env-entry-name>file</env-entry-name>
       <env-entry-type>java.lang.String</env-entry-type>
       <env-entry-value>ServerSideConfig.properties</env-entry-value>
     </env-entry>
     <env-entry>
       <env-entry-name>namespace</env-entry-name>
       <env-entry-type>java.lang.String</env-entry-type>
       <env-entry-value>ResourceManagerServiceBean</env-entry-value>
     </env-entry>
     <env-entry>
       <env-entry-name>resourceManagerKey</env-entry-name>
       <env-entry-type>java.lang.String</env-entry-type>
       <env-entry-value>resourceManagerKey</env-entry-value>
     </env-entry>
     <env-entry>
       <env-entry-name>cacheResourceRoles</env-entry-name>
       <env-entry-type>java.lang.Boolean</env-entry-type>
       <env-entry-value>false</env-entry-value>
     </env-entry>
     <env-entry>
       <env-entry-name>cacheNotificationTypes</env-entry-name>
       <env-entry-type>java.lang.Boolean</env-entry-type>
       <env-entry-value>false</env-entry-value>
     </env-entry>
   </session>


  </enterprise-beans>

</ejb-jar>
