<?xml version="1.0" ?>
<CMConfig>
    <Config name="com.topcoder.search.builder.SearchBundle">
        <Property name="searchStrategyFactoryNamespace">
            <Value>com.topcoder.search.builder.strategy.factory</Value>
        </Property>

        <Property name="fieldValidatorFactoryNamespace">
            <Value>com.topcoder.search.builder.validator.factory</Value>
        </Property>

        <Property name="searchBundles">

            <Property name="DbRejectEmailSearchBundle">
                <Property name="searchStrategy">
                    <Property name="class">
                        <Value>dbStrategy</Value>
                    </Property>
                </Property>
                <!-- please do not modify the context (at least the order of the SELECT list) -->
                <Property name="context">
                    <Value>
                        SELECT
                        reject_email.reject_email_id,
                        comp_rej_email.company_id,
                        reject_email.body,
                        reject_email.creation_date,
                        reject_email.creation_user,
                        reject_email.modification_date,
                        reject_email.modification_user
                        FROM
                        reject_email,
                        comp_rej_email
                        WHERE
                        reject_email.reject_email_id = comp_rej_email.reject_email_id
                        AND
                    </Value>
                </Property>
                <!-- please do not modify the alies -->
                <Property name="alias">
                    <Property name="reject_email_id">
                        <Value>reject_email.reject_email_id</Value>
                    </Property>
                    <Property name="search_company_id">
                        <Value>comp_rej_email.company_id</Value>
                    </Property>
                    <Property name="search_body">
                        <Value>reject_email.body</Value>
                    </Property>
                    <Property name="search_created_date">
                        <Value>reject_email.creation_date</Value>
                    </Property>
                    <Property name="search_created_user">
                        <Value>reject_email.creation_user</Value>
                    </Property>
                    <Property name="search_modification_date">
                        <Value>reject_email.modification_date</Value>
                    </Property>
                    <Property name="search_modification_user">
                        <Value>reject_email.modification_user</Value>
                    </Property>
                </Property>
                <Property name="searchableFields">
                    <Property name="reject_email_id">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_company_id">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_body">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_created_date">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_created_user">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_modification_date">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                    <Property name="search_modification_user">
                        <Property name="validator">
                            <Property name="class">
                                <Value>validator</Value>
                            </Property>
                            <Property name="identifier">
                                <Value>default</Value>
                            </Property>
                        </Property>
                    </Property>
                </Property>
            </Property>

            <Property name="DbRejectReasonSearchBundle">
