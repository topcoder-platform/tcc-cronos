<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="dist" name="Dependency Import">
    <!--property file defining the component's dependencies-->
    <!--Properties used in compile_targets ANT target for JDK version x-->
    <property name="component.target" value="1.5"/>
    <property name="component.bootclasspath" value="${java_1_5_bootclasspath}"/>

    <!--TCS dependencies-->
    <property name="base_exception.jar" value="${tcs_libdir}/base_exception/2.0.0/base_exception.jar"/>
		<property name="configuration_api.jar" value="${tcs_libdir}/configuration_api/1.0.0/configuration_api.jar" />
		<property name="logwrapper.jar" value="${tcs_libdir}/logging_wrapper/2.0.0/logging_wrapper.jar" />
		<property name="configuration_manager.jar" value="${tcs_libdir}/configuration_manager/2.1.5/configuration_manager.jar"/>
		<property name="document_generator.jar" value="${tcs_libdir}/document_generator/2.1.0/document_generator.jar" />
		<property name="email_engine.jar" value="${tcs_libdir}/email_engine/3.2.0/email_engine.jar" />
		<property name="file_upload.jar" value="${tcs_libdir}/file_upload/2.2.0/file_upload.jar" />
		<property name="object_formatter.jar" value="${tcs_libdir}/object_formatter/1.0.0/object_formatter.jar" />
		<property name="member_photo_manager.jar" value="test_files/ref_lib/member_photo_manager.jar" />
		<property name="typesafe_enum.jar" value="${tcs_libdir}/typesafe_enum/1.1.0/typesafe_enum.jar" />
		<property name="guid_generator.jar" value="${tcs_libdir}/guid_generator/1.0.1/guid_generator.jar" />
		
    <!--3rd party dependencies-->
    <property name="junit.jar" value="${ext_libdir}/junit/4.6/junit-4.6.jar"/>
		<property name="spring.jar" value="${ext_libdir}/spring/2.5.6/dist/spring.jar" />
		<property name="spring-test.jar" value="${ext_libdir}/spring/2.5.6/dist/modules/spring-test.jar" />
		<property name="javaee.jar" value="${ext_libdir}/javaee/5.0/javaee.jar" />
		<property name="activation.jar" value="${ext_libdir}/jaf/1.1/activation-1.1.jar" />
		<property name="javamail.jar" value="${ext_libdir}/javamail/1.4.3/mail.jar" />
		<property name="commons-logging.jar" value="${ext_libdir}/commons-logging/1.1.1/commons-logging-1.1.1.jar" />
		<property name="log4j.jar" value="${ext_libdir}/log4j/1.2.15/log4j-1.2.15.jar" />
		<property name="httpunit.jar" value="${ext_libdir}/httpunit/1.6.2/lib/httpunit.jar" />

    <!--Path declarations which are to be used in the master build file.-->
    <path id="component.tcs-dependencies">
        <pathelement location="${base_exception.jar}"/>
        <pathelement location="${configuration_api.jar}"/>
        <pathelement location="${configuration_manager.jar}"/>
        <pathelement location="${document_generator.jar}"/>
        <pathelement location="${email_engine.jar}"/>
        <pathelement location="${file_upload.jar}"/>
        <pathelement location="${member_photo_manager.jar}"/>
		    <pathelement location="${logwrapper.jar}"/>
		    <pathelement location="${object_formatter.jar}"/>
		    <pathelement location="${typesafe_enum.jar}"/>
		    <pathelement location="${guid_generator.jar}"/>
    </path>
    <path id="component.3rdParty-dependencies">     
        <pathelement location="${javaee.jar}"/>
        <pathelement location="${spring.jar}"/>
        <pathelement location="${spring-test.jar}"/>
        <pathelement location="${javamail.jar}"/>
        <pathelement location="${httpunit.jar}"/>
    </path>
    <path id="component.test.3rdParty-dependencies">
        <pathelement location="${junit.jar}"/>
        <pathelement location="${activation.jar}"/>
        <pathelement location="${commons-logging.jar}"/>
        <pathelement location="${log4j.jar}"/>
    </path>
</project>