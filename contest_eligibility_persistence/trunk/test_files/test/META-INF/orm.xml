<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="1.0"
	xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">

	<entity class="com.topcoder.service.contest.eligibility.ContestEligibility">
		<table name="contest_eligibility">
		</table>
		<inheritance strategy="JOINED" />
		<sequence-generator name="contest_eligibility_gen"
			sequence-name="CONTEST_ELIGIBILITY_SEQ" />
		<attributes>
			<id name="id">
				<generated-value strategy="SEQUENCE" generator="contest_eligibility_gen" />

			</id>
			<basic name="contestId">
				<column name="contest_id" />
			</basic>
			<basic name="studio">
				<column name="is_studio" />
			</basic>
			<transient name="deleted" />
		</attributes>
	</entity>


	<entity class="com.topcoder.service.contest.eligibility.GroupContestEligibility">
		<table name="group_contest_eligibility">
		</table>
		<attributes>
			<basic name="groupId">
				<column name="group_id" />
			</basic>
		</attributes>
	</entity>
</entity-mappings>
