\par                AND PT.PROJECTSID = PR.PROJECTSID
\par                AND PW.PROJECTSID = PR.PROJECTSID
\par                AND CL.CLIENTSID = CP.CLIENTSID
\par                AND CP.PROJECTSID = PR.PROJECTSID
\par                AND US.USEDRSID = PW.USERSID
\par                AND US.NAME = TE.CREATIONUSER
\par          </Value>
\par       </Property>
\par 
\par }\pard \ql \fi720\li720\ri0\nowidctlpar\intbl\faauto\rin0\lin720\pararsid10776890\yts15 {\f2\fs20\cf9\insrsid10776890\charrsid14509625 <Property name="FILTER_EMPLOYEE_EMPLOYEE_TIME">
