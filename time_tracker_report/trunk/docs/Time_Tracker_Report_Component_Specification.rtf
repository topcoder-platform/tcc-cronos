\par }\pard \ql \li720\ri0\nowidctlpar\intbl\faauto\rin0\lin720\pararsid10776890\yts15 {\f2\fs18\cf9\insrsid10776890 
\par 
\par <!-- Query Configuration and FilterExtensions for the "Employee Time Report" -->
\par       
\par       <Property name="BASE_QUERY_EMPLOYEE_TIME">
\par          <Value>
\par             SELECT TE.ENTRYDATE DATE,
\par                    CL.NAME CLIENT,
\par                    PR.NAME PROJECT,
\par                    TT.DESCRIPTION TASK,
\par                    TE.HOURS HOURS,
\par                    PW.PAYRATE PAY_RATE,
\par                    TE.BILLABLE BILLABLE
\par 
\par               FROM TIMEENTRIES TE,
\par                    TASKTYPES TT,
\par                    CLIENTS CL,
\par                    PROJECTTIMES PT,
\par                    PROJECTS PR,
\par                    CLIENTPROJECTS CP,
\par                    PROJECTWORKERS PW,
\par                    USERS US
\par 
\par              WHERE TE.TASKTYPESID = TT.TASKTYPESID
\par                AND PT.TIMEENTRIESID = TE.TIMEENTRIESID
\par                AND PT.PROJECTSID = PR.PROJECTSID
\par                AND PW.PROJECTSID = PR.PROJECTSID
\par                AND CL.CLIENTSID = CP.CLIENTSID
\par                AND CP.PROJECTSID = PR.PROJECTSID
\par                AND US.USEDRSID = PW.USERSID
\par                AND US.NAME = TE.CREATIONUSER
\par          </Value>
\par       </Property>
\par 
\par }\pard \ql \fi720\li720\ri0\nowidctlpar\intbl\faauto\rin0\lin720\pararsid10776890\yts15 {\f2\fs18\cf9\insrsid10776890\charrsid5767264 <Property name="FILTER_EMPLOYEE_EMPLOYEE_TIME">
\par }\pard \ql \li720\ri0\nowidctlpar\intbl\faauto\rin0\lin720\pararsid10776890\yts15 {\f2\fs18\cf9\insrsid10776890\charrsid5767264          <Value>TE.CREATIONUSER = ?</Value>
\par       </Property>      }{\f2\fs18\cf9\insrsid10776890 
\par }{\f2\fs18\cf9\insrsid10776890\charrsid5767264 
\par }{\f2\fs18\cf9\insrsid10776890       <Property name="FILTER_DATE_EMPLOYEE_TIME">
\par          <Value>TE.ENTRYDATE BETWEEN ? AND ?</Value>
\par       </Property>
\par 
\par       <Property name="FILTER_CLIENT_EMPLOYEE_TIME">
\par          <Value>CL.NAME = ?</Value>
\par       </Property>
\par 
\par       <Property name="FILTER_PROJECT_EMPLOYEE_TIME">
\par          <Value>PR.NAME = ?</Value>
\par       </Property>
\par 
\par       <Property name="FILTER_BILLABLE_EMPLOYEE_TIME">
\par          <Value>TE.BILLABLE = ?</Value>
\par       </Property>
