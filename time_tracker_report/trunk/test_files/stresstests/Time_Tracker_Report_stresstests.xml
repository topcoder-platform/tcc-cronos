         <Value>
            SELECT TE.ENTRYDATE DATE,
                   CL.NAME CLIENT,
                   PR.NAME PROJECT,
                   TE.HOURS HOURS,
                   -1 AMOUNT,
                   TE.BILLABLE BILLABLE

              FROM TIMEENTRIES TE,
                   CLIENTS CL,
                   PROJECTTIMES PT,
                   PROJECTS PR,
                   CLIENTPROJECTS CP

             WHERE PT.TIMEENTRIESID = TE.TIMEENTRIESID
               AND PT.PROJECTSID = PR.PROJECTSID
               AND CL.CLIENTSID = CP.CLIENTSID
               AND CP.PROJECTSID = PR.PROJECTSID

            UNION

            SELECT EE.ENTRYDATE DATE,
                   CL.NAME CLIENT,
                   PR.NAME PROJECT,
                   -1 HOURS,
                   EE.AMOUNT AMOUNT,
