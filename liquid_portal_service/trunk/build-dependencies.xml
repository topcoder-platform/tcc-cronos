<project name="Dependency Import" default="" basedir=".">
	<!-- property file defining the component's dependencies -->

	<!-- Properties used in compile_targets ANT target for JDK version x -->
	<property name="component.target" value="1.5" />
	<property name="component.bootclasspath" value="${java_1_5_bootclasspath}" />
	
	<!-- TCS dependencies -->
	<property name="base_exception.jar" value="${tcs_libdir}/base_exception/2.0.0/base_exception.jar" />
	<property name="catalog_entities.jar" value="${cronos_libdir}/catalog_entities/1.2.0/catalog_entities.jar" />
	<property name="client_project_entities_dao.jar" value="${cronos_libdir}/client_project_entities_dao/1.1.0/client_project_entities_dao.jar" />	
	<property name="configuration_manager.jar" value="${tcs_libdir}/configuration_manager/2.1.5/configuration_manager.jar" />
	<property name="configuration_api.jar" value="${tcs_libdir}/configuration_api/1.0.0/configuration_api.jar" />
  <property name="configuration_persistence.jar" value="${tcs_libdir}/configuration_persistence/1.0.1/configuration_persistence.jar" />
	<property name="contest_service_facade.jar" value="${cronos_libdir}/contest_service_facade/1.1.0/contest_service_facade.jar" />
	<property name="document_generator.jar" value="${tcs_libdir}/document_generator/3.0.0/document_generator.jar" />
	<property name="email_engine.jar" value="${tcs_libdir}/email_engine/3.1.0/email_engine.jar" />
	<property name="logging_wrapper.jar" value="${tcs_libdir}/logging_wrapper/2.0.0/logging_wrapper.jar" />
	<property name="object_formatter.jar" value="${tcs_libdir}/object_formatter/1.0.0/object_formatter.jar" />
	<property name="pipeline_service.jar" value="${cronos_libdir}/pipeline_service/1.0.0/pipeline_service.jar" />	
	<property name="pipeline_service_facade.jar" value="${cronos_libdir}/pipeline_service_facade/1.0.0/pipeline_service_facade.jar" />	
	<property name="project_service.jar" value="${cronos_libdir}/project_service/1.1.0/project_service.jar" />	
	<property name="studio_service.jar" value="${cronos_libdir}/studio_service/1.3.0/studio_service.jar" />	
	<property name="typesafe_enum.jar" value="${tcs_libdir}/typesafe_enum/1.1.0/typesafe_enum.jar" />
			
	
	<!-- 3rd party dependencies -->
	<property name="junit.jar" value="${ext_libdir}/junit/3.8.2/junit.jar" />
	<property name="javaee.jar" value="${ext_libdir}/j2ee/5.0/javaee.jar" />


<path id="component.tcs-dependencies">
	<pathelement location="${base_exception.jar}"/>
	<pathelement location="${catalog_entities.jar}"/>
	<pathelement location="${client_project_entities_dao.jar}"/>	
	<pathelement location="${configuration_manager.jar}"/>
	<pathelement location="${configuration_api.jar}"/>
  <pathelement location="${configuration_persistence.jar}"/>
	<pathelement location="${contest_service_facade.jar}"/>
	<pathelement location="${document_generator.jar}"/>
	<pathelement location="${email_engine.jar}"/>
	<pathelement location="${logging_wrapper.jar}"/>
	<pathelement location="${object_formatter.jar}"/>
	<pathelement location="${pipeline_service.jar}"/>	
	<pathelement location="${pipeline_service_facade.jar}"/>	
	<pathelement location="${project_service.jar}"/>	
	<pathelement location="${studio_service.jar}"/>	
	<pathelement location="${typesafe_enum.jar}"/>
</path>

	<path id="component.3rdParty-dependencies">
		<pathelement location="${javaee.jar}" />
	</path>

	<path id="component.test.3rdParty-dependencies">
		<pathelement location="${junit.jar}" />
	</path>

</project>