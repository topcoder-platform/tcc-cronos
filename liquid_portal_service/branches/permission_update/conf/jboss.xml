<?xml version="1.0" encoding="UTF-8"?>
<jboss>
  <enterprise-beans>
    <session>
        <ejb-name>LiquidPortalServiceBean</ejb-name>
        <jndi-name>remote/LiquidPortalServiceBean</jndi-name>

        <ejb-ref>
                <ejb-ref-name>ejb/CatalogService</ejb-ref-name>
                <jndi-name>CatalogService/remote</jndi-name>
       </ejb-ref>
       <ejb-ref>
               <ejb-ref-name>ejb/PermissionService</ejb-ref-name>
                <jndi-name>PermissionServiceBean/remote</jndi-name>
       </ejb-ref>

        <port-component>
				<port-component-name>LiquidPortalServiceBean</port-component-name>
				<port-component-uri>/LiquidPortalServiceBean</port-component-uri>
				<transport-guarantee>CONFIDENTIAL</transport-guarantee>
		</port-component>

    </session>
  </enterprise-beans>
    <security-domain>java:/jaas/JBossLoginModule</security-domain>
	<webservices> 
		<context-root>/liquidportalservice</context-root> 
		<webservice-description> 
		<webservice-description-name />
		<wsdl-publish-location />
    </webservice-description>
 </webservices>
</jboss>
