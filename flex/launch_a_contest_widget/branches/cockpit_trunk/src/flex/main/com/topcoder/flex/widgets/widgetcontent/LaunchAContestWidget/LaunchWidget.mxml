                    case 4:


                       specf.requestFileTypes.send();
                       this.currentState = "SpecificationsState";
                       setReadOnlyMode();
                       save.includeInLayout=save.visible=!isReadOnlyMode;
                        if (save.visible) {
                            save.label=getSaveLabel();
                        }

                        goNext.includeInLayout=goNext.visible=true;
