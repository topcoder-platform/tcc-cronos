package com.topcoder.flex.widgets.widgetcontent.LaunchAContestWidget.webservice.data.software.catalog {

    /**
    * A Software Technology DTO class.
    * 
    * @author TCSDEVELOPER
    * 
    * @since Flex Cockpit Launch Contest - Integrate Software Contests v1.0
    */ 
    public class SoftwareTechnology {

        /**
        * A default and empty constructor.
        */
        public function SoftwareTechnology() {
        }

        /**
        * Identifier of this technology.
        */
        public var id:Number=0;

        /**
        * Name of this technology.
        */
        public var name:String=null;

        /**
        * Description of this technology.
        */
        public var description:String=null;

        /**
        * Status of this technology.
        */
        public var status:String=null;

    }
}