<CMConfig>
	<Config name="com.topcoder.db.connectionfactory.DBConnectionFactoryImpl">
		<Property name="connections">
			<Property name="default">
				<Value>IDGeneratorImpl</Value>
			</Property>
			<Property name="IDGeneratorImpl">
				<Property name="producer">
					<Value>com.topcoder.db.connectionfactory.producers.JDBCConnectionProducer</Value>
				</Property>
				<Property name="parameters">
					<Property name="jdbc_driver">
						<Value>com.microsoft.sqlserver.jdbc.SQLServerDriver</Value>
					</Property>
					<Property name="jdbc_url">
						<Value>jdbc:sqlserver://localhost:1433</Value>
					</Property>
					<Property name="databaseName">
						<Value>orpheus</Value>
					</Property>
					
					<Property name="user">
						<Value>sa</Value>
					</Property>
					
					<Property name="password">
						<Value>topcoder</Value>
					</Property>

					<Property name="selectMethod">
						<Value>cursor</Value>
					</Property>

	
				</Property>
			</Property>
		</Property>
	</Config>
</CMConfig>
