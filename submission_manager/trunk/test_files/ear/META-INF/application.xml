        <ejb>submission_manager.jar</ejb>
    </module>
    <module>
        <java>contest_and_submission_entities.jar</java>
    </module>
    <module>
        <java>base_exception.jar</java>
    </module>
    <module>
        <java>logging_wrapper.jar</java>
    </module>
    <module>
        <java>object_formatter.jar</java>
    </module>
    <module>
        <java>typesafe_enum.jar</java>
    </module>
    <module>
        <java>ifxjdbc.jar</java>
    </module>
    <module>
        <java>informix-ds.xml</java>
    </module>
</application>
