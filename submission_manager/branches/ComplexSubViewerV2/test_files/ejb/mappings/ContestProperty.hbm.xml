<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.topcoder.service.studio.contest">
	<class name="ContestProperty" table="contest_property_lu" lazy="true">
		<id name="propertyId" column="property_id">
			<generator class="sequence">
				<param name="sequence">sq_contest_property_lu</param>
			</generator>
		</id>
		<property name="description" column="property_desc"/>
	</class>
</hibernate-mapping>